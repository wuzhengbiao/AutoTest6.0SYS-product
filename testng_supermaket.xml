<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="test">
    <listeners>
        <listener class-name="org.uncommons.reportng.HTMLReporter"/>
        <listener class-name="org.uncommons.reportng.JUnitXMLReporter" />
        <listener class-name="BasicMethods.AssertionListener" />
        <listener class-name="BasicMethods.OverrideIAnnotationTransformer"/>
    </listeners>
    <test name="课程超市6.0进班学习">
        <parameter name="TestFilePath" value="/TestList/6.0Supermarketstudy.xlsx"/>
        <parameter name="TIME" value="20"/>
        <parameter name="TestName" value="6.0Supermarketstudy.xlsx"/>
        <classes>
            <class name="Testing.Tester"/>
        </classes>
    </test>
    <test name="课程超市6.0自主选课学习">
        <parameter name="TestFilePath" value="/TestList/6.0SupermarketAutonomousLearning.xlsx"/>
        <parameter name="TIME" value="20"/>
        <parameter name="TestName" value="6.0SupermarketAutonomousLearning.xlsx"/>
        <classes>
            <class name="Testing.Tester"/>
        </classes>
    </test>
 </suite>